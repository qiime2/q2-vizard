{
  "$schema": "https://vega.github.io/schema/vega/v5.json",
  "title": {
    "text": {"signal": "title"},
    "fontSize": 20,
    "orient": "top",
    "anchor": "start",
    "subtitle": {"signal": "suppressOutliers ? 'NOTE: Outliers have been suppressed.' : ' '"}
  },
  "autosize": {
    "type": "pad",
    "resize": true
  },
  "width": 900,
  "height": {"signal": "rowHeight * nRows"},
  "padding": 20,

  "signals": [
    {
      "name": "title",
      "value": {"{{REPLACE_PARAM}}": "title"},
      "bind": {"input": "input"}
    },
    {
      "name": "xField",
      "value": {"{{REPLACE_PARAM}}": "distribution_measure"}
    },
    {
      "name": "yField",
      "value": {"{{REPLACE_PARAM}}": "group_by"}
    },
    {
      "name": "whiskerMethod",
      "value": {"{{REPLACE_PARAM}}": "whisker_range"}
    },
    {
      "name": "transpose",
      "value": false,
      "bind": {"input": "checkbox"}
    },
    {
      "name": "suppressOutliers",
      "value": false,
      "bind": {"input": "checkbox"}
    },
    {
      "name": "nRows",
      "value": 1,
      "update": "length(data('rowTable'))"
    },
    {
      "name": "rowHeight",
      "value": 45,
      "bind": {
        "input": "range",
        "min": 5,
        "max": 200
      }
    }
  ],

  "data": [
    {
      "name": "table",
      "values": {"{{REPLACE_PARAM}}": "metadata"}
    },
    {
      "name": "rowTable",
      "source": "table",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [{"{{REPLACE_PARAM}}": "group_by"}]
        }
      ]
    },
    {
      "name": "percentiles_table",
      "source": "table",
      "transform": [
        {
          "type": "quantile",
          "field": {"{{REPLACE_PARAM}}": "distribution_measure"},
          "probs": [0.09, 0.91],
          "groupby":[{"{{REPLACE_PARAM}}": "group_by"}],
          "as": ["prob", "value"]
        }
      ]
    },
    {
      "name": "p09_data",
      "source": "percentiles_table",
      "transform": [
        {
          "type": "filter",
          "expr": "datum.prob == 0.09"
        }
      ]
    },
    {
      "name": "p91_data",
      "source": "percentiles_table",
      "transform": [
        {
          "type": "filter",
          "expr": "datum.prob == 0.91"
        }
      ]
    },
    {
      "name": "summary",
      "source": "table",
      "transform": [
        {
          "type": "aggregate",
          "groupby": [{"{{REPLACE_PARAM}}": "group_by"}],
          "fields": [
            {"{{REPLACE_PARAM}}": "distribution_measure"},
            {"{{REPLACE_PARAM}}": "distribution_measure"},
            {"{{REPLACE_PARAM}}": "distribution_measure"},
            {"{{REPLACE_PARAM}}": "distribution_measure"},
            {"{{REPLACE_PARAM}}": "distribution_measure"}
          ],
          "ops": ["min","q1", "median", "q3", "max"],
          "as": ["min", "q1", "median", "q3", "max"]
        },
        {
          "type": "formula",
          "as": "iqr",
          "expr": "datum.q3 - datum.q1"
        },
        {
          "type": "lookup",
          "from": "p09_data",
          "key": {"{{REPLACE_PARAM}}": "group_by"},
          "fields": [{"{{REPLACE_PARAM}}": "group_by"}],
          "values": ["value"],
          "as": ["p09"]
        },
        {
          "type": "lookup",
          "from": "p91_data",
          "key": {"{{REPLACE_PARAM}}": "group_by"},
          "fields": [{"{{REPLACE_PARAM}}": "group_by"}],
          "values": ["value"],
          "as": ["p91"]
        },
        {
          "type": "formula",
          "as": "whiskerLow",
          "expr": "whiskerMethod == 'tukeys_iqr' ? max(datum.q1 - 1.5 * datum.iqr, datum.min) : whiskerMethod == 'percentile' ? datum.p09 : datum.min"
        },
        {
          "type": "formula",
          "as": "whiskerHigh",
          "expr": "whiskerMethod == 'tukeys_iqr' ? min(datum.q3 + 1.5 * datum.iqr, datum.max) : whiskerMethod == 'percentile' ? datum.p91 : datum.max"
        }
      ]
    },
    {
      "name": "outliers",
      "source": "table",
      "transform": [
        {
          "type": "lookup",
          "from": "summary",
          "key": {"{{REPLACE_PARAM}}": "group_by"},
          "fields": [{"{{REPLACE_PARAM}}": "group_by"}],
          "as": ["summary"]
        },
        {
          "type": "filter",
          "expr": {"{{REPLACE_PARAM}}": "expr"}
        }
      ]
    }

  ],

  "scales": [
    {
      "name": "x_scale",
      "type": "linear",
      "range": {"signal": "transpose ? [0, height] : [0, width]"},
      "domain": {
        "data": "table",
        "field": {"{{REPLACE_PARAM}}": "distribution_measure"}},
      "nice": true
    },
    {
      "name": "y_scale",
      "type": "band",
      "range": {"signal": "transpose ? [0, width] : [0, height]"},
      "domain": {
        "data": "table",
        "field": {"{{REPLACE_PARAM}}": "group_by"}}
    },
    {
      "name": "color_scale",
      "type": "ordinal",
      "domain": {
        "data": "table",
        "field": {"{{REPLACE_PARAM}}": "group_by"},
        "sort": true
      },
      "range": {"scheme": "category10"}
    }
  ],

  "axes": [
    {
      "scale": "x_scale",
      "labelFontSize": 12,
      "orient": {"signal": "transpose ? 'left' : 'bottom'"},
      "title": {"signal": "transpose ? yField : xField"},
      "titleFontSize": 14,
      "titlePadding": 20
    },
    {
      "scale": "y_scale",
      "labelFontSize": 12,
      "grid": true,
      "orient": {"signal": "transpose ? 'bottom' : 'left'"},
      "tickCount": {"signal": "nRows"},
      "title": {"signal": "transpose ? xField : yField"},
      "titleFontSize": 14,
      "titlePadding": 20
    }
  ],

  "legends": [
    {
      "stroke": "color_scale",
      "symbolSize": 150,
      "symbolStrokeWidth": 2.5,
      "labelFontSize": 14,
      "title": {"signal": "yField"},
      "titleFontSize": 16,
      "encode": {
        "symbols": {
          "name": "legendSymbol",
          "interactive": false
        },
        "labels": {
          "name": "legendLabel",
          "interactive": false
        }
      }
    }
  ],

  "marks": [

  ]
}
